{
  "presets": [
    [
    "@babel/env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    [
      "@babel/react"
    ]
  ],
  "plugins": [
    "@babel/transform-runtime",
    "@babel/proposal-class-properties",
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "style": "css"
      }
    ]
  ]
}
